{
  "name": "coffee-backend",
  "version": "1.0.0",
  "description": "Express Server with MongoDB for my-coffee-app",
  "main": "./src/server.js",
  "type": "module",
  "scripts": {
    "test": "jest --coverage",
    "start": "node -r dotenv/config ./dist/server.js",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "dev": "node --watch --loader ts-node/esm ./src/server.ts",
    "dev-old": "ts-node-dev --respawn --pretty --transpile-only -r dotenv/config ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mrbusy13/Coffee-backend.git"
  },
  "author": "mrbusy and NovemberGolf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mrbusy13/Coffee-backend/issues"
  },
  "homepage": "https://github.com/Mrbusy13/Coffee-backend#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.4.3",
    "mongodb": "^5.0.1",
    "mongoose": "^6.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/parser": "^5.53.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
